{
	"input" : {		
 		"tsdb-in" : {
 			"port" : "4242",
 			"type" : "metric"
 		},"collectd-in" : {
 			"type" : "metric",
			"add-tag" : "collectd"
 		},"self":{ "type" :"self","interval":5000}	
 	
	},"filter":{
		"batching":{
			"type":"metric",
			"interval":500,
			"maxbatch":1000,
			"out-type":"output",
			"out-add-tag" : ["batched"]
		},
		"json-encoder" : {
			"tags":["batched"],
 			"add-tag" : ["json"],
 			"field" : "json",
 			"prio":500
 		},"LZ4-compress":{
 			"infield":"json",
 			"outfield":"body",
 			"prio":600,
 			"tags" : ["json"]
 		},"mvel":{
 			"type":"self",
 			"script":"if( !metric.startsWith('flens.LZ')){DISCARD=true} return null; "
 		}
	},
 	
 	"output" : {
 		"system-out" : {
 			"type" : "self"
 			
 			
 		}
 		
 	}
 		 

}
