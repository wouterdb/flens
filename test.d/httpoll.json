{"init":
{
	"input":{
		"http-poll":{
			"url":"http://172.16.4.21:8123/livestats",
			"type":"metric",
			"add-tag":["json","packed","http-in"]
		}
	},"filter":{
		"http-json-decoder":{
			"plugin":"json-decoder",
			"tags" : ["json","http-in"],
 			"remove-tag" : ["json"],
 			"prio":1,
 			"type":"metric"
 			
		},"http-tp":{
			"plugin":"template",
		    "prio":2,
			"type":"metric",
			"field":["metric","value","message"],
			"template":["jobs","@{jobs}","@{null}"],
			"tags":"http-in"
		},"http-split":{
			"plugin":"splitter",
			"fields":["stats"],
			"tags" : ["packed","http-in"],
 			"remove-tag" : ["packed","http-in"],
 			"add-tag": ["http-new"],
			"prio":3,
			"keep":true,
			"type":"metric"
		},"http-tp2":{
			"plugin":"template",
			"tags":["http-new"],
		    "prio":4,
			"field":["metric","workerType"],
			"template":["queue-size","@{metric}"]
		}
	},"output":{
		"system-out":{
		}
	}
}

}