{
	"input" : {
 		"system-in" : { 
 			"name" : "in",
 			"type" : "log",
 			"add-tag" : "raw"
 		},
 		
 		"tsdb-in" : {
 			"name" : "tsdb",
 			"port" : "4242",
 			"type" : "metric"
 		},
 		
 		"self" : {
 			"interval":1000,
 			"name" : "self",
 			"type" : "metric",
 			"add-tag" : ["raw","packed"]
 		}
 	},
 	
 	"filter" : {
 		"unpacker" : {
 			"type" : "metric",
 			"tags" : ["packed"],
 			"remove-tag" : ["packed"]
 		}
 	},
 	
 	"output" : {
 		"system-out" : {
 			"name" : "out",
 			"type" : "metric"
 		}
 	}
 		 

}